pipeline {
    parameters{
    	choice(NAME, choices: ['build', 'teste'], description: 'Qual o stage pรก?')
    }
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
    stages {
    	stage('"$(params.CHOICE)"')
	 steps {
	  sh 'echo build'	
	 }
	 stage('"$(params.CHOICE)"') {
	 	steps {
	 	sh 'echo teste'
	}
    }
}
