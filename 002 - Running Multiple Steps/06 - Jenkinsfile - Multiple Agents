pipeline {
    agent none
    tools {
        maven 'apache-maven-3.0.1' 
    }
        tools {
        npm 'npm-3.0.1' 
    }
    stages {
        stage('Maven Version') {
		steps{
			sh 'mvn --version'
		}
                
        }
	stage('NPM Version') {
                steps{
			sh 'npm --version'
		}
        }
    }
}
